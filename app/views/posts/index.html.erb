                  <% else %>
                    N/A
                  <% end %>
              </td>
                <td style="padding-top:43px;">
                  <div style="width:50px;margin:0 auto;">
                    <% if post.user.image %>
                      <%= link_to image_tag(post.user.image, style: :'border-radius:25px;'), user_path(post.user) %>
                    <% else %>
                      <%= link_to image_tag('member_head.png', style: :'width:50px;border-radius:25px;'), user_path(post.user) %>
                    <% end %>
                  </div>
                </td>
                <td style="text-align:center; padding-top:38px;">
                  <%= post.start_date %><br>
                  <div style="margin:0 auto; width:2px;">|</div>
                  <%= post.return_date %>
                </td>
                <td> post.country </td>
                <td style="text-align:center; padding-top:61px;"><%= post.destination %></td>
                <td style="text-align:center; padding-top:61px;"><%= post.origin %></td>
                <td style="text-align:center; padding-top:61px;"><%= post.distance %></td>
                <td style="text-align:center; padding-top:58px;">
                  <% favorite = post.find_my_favorite(current_user) %>
                    <div id="favorite_block_<%= post.id %>" style="display:inline; margin-right:20px;">
                      <%#= render :partial => "favorites/favorite", :locals => { :favorite => favorite, :post => @post } %>
                      <% if favorite %>
                        <%= link_to image_tag('album_sub_love_hover.png', :'data-toggle'=>'tooltip', :'data-placement'=>'auto bottom', :title=>'將此相簿加入我的最愛'), post_favorite_path(post, favorite), :method => :delete, :locals => { :favorite => favorite }, :remote => true %>
                      <% else %>
                        <%= link_to image_tag('album_sub_love.png', :'data-toggle'=>'tooltip', :'data-placement'=>'auto bottom', :title=>'將此相簿加入我的最愛'), post_favorites_path(post), :method => :post, :remote => true %>
                      <% end %>
                      <% if post.favorites.count > 0 %>
                        <%= post.favorites.count %> 
                      <% else %>
                      <% end %>
                    </div>
                </td>
                <td style="text-align:center; padding-top:52px;">
                  <% if current_user && post.user == current_user %>
                    <div class="post_locker_<%= post.id %>" style="display:inline;">
                      <%#= render :partial => "locker", :locals => { :post => @post } %>
                      <% if post.status == 'locked' %>
                        <%= link_to image_tag("album_sub_lock.png",alt: :"sub_lock", style: :"margin-right:20px"),change_status_post_path(post,:status => :'published'), :method => :post, remote: :true %>
                      <% else %>
                        <%= link_to image_tag("album_sub_open.png",alt: :"sub_lock", style: :"margin-right:20px"), change_status_post_path(post, :status=> 'locked'), :method => :post, remote: :true %>
                      <% end %>
                    </div>
                  </td>
                  <td style="text-align:center; padding-top:52px;">
                    <div class="post_trashcan_<%= post.id %>" style="display:inline;">
                      <%#= render :partial => "trashcan", :locals => { :post => @post } %>
                      <% if post.status == 'trashcan' %>
                        <%= link_to image_tag("album_sub_trash_hover.png",alt: :"sub_trashcan"),change_status_post_path(post,:status => :'draft'),:method => :post, remote: :true %>
                      <% else %>
                        <%= link_to image_tag("album_sub_trash.png",alt: :"sub_trashcan"),change_status_post_path(post, :status=> 'trashcan'), :method => :post, remote: :true %>
                      <% end %>
                    </div>
                  <% end %>
                </td>
              </tr>
            <% end %>    <!-- 458 -->
          </tbody>
        </table>
      </div>

    </div>
  <br><br>
</div>

</div>        <!-- container -->

<script>
  $('#myTabs a').click(function (e) {
    e.preventDefault()
    $(this).tab('show')
  })
</script>